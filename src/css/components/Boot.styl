$px = 8px

.Boot
  @extends .VT323
  color white
  font-size 40px
  text-transform uppercase

  height 100%
  > div
    height 100%
    display flex
    justify-content center
    align-items center

  span
    position absolute

  .Output
    iframe
      width 384px
      height @width
      
    @media screen and (max-width: 416px)
      padding 0
      iframe
        width 100vw
        height 100vw
