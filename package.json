{
  "name": "gametron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "build-css": "stylus src/css/App.styl -o src/css",
    "watch-css": "stylus src/css/App.styl -o src/css -w",
    "start-js": "react-scripts start",
    "startx": "npm-run-all -p watch-css start-js",
    "start": "npm-run-all -p start-js",
    "build-js": "react-scripts build",
    "buildx": "npm-run-all build-css build-js",
    "build": "npm-run-all build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "standard": {
    "globals": [
      "it"
    ]
  }
}
