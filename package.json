{
  "name": "script-8",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "acorn": "^5.4.1",
    "chokidar": "^2.0.2",
    "chokidar-cli": "^1.2.1",
    "classnames": "^2.2.5",
    "d3": "^4.13.0",
    "d3-scale": "^2.0.0",
    "d3-timer": "^1.0.7",
    "deep-equal": "^1.0.1",
    "gh-pages": "^1.1.0",
    "github-api": "^3.0.0",
    "javascript-state-machine": "^3.0.1",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.5",
    "lz-string": "^1.4.4",
    "npm-run-all": "^4.1.2",
    "omit-empty": "^0.4.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "stylus": "^0.54.5",
    "tone": "^0.12.80",
    "twas": "^2.0.0",
    "ws": "^4.1.0"
  },
  "homepage": "http://script-8.github.io",
  "scripts": {
    "watch-help": "chokidar \"src/utils/help.yaml\" -c \"npm run parse-help\"",
    "parse-help": "js-yaml src/utils/help.yaml > src/utils/help.json",
    "watch-lessons": "chokidar \"src/utils/lessons.yaml\" -c \"npm run parse-lessons\"",
    "parse-lessons": "js-yaml src/utils/lessons.yaml > src/utils/lessons.json",
    "timestamp": "date '+ {\"timestamp\":\"%H:%M %d/%m/%y\"}' > src/utils/timestamp.json; git commit -m 'timestamp' src/utils/timestamp.json",
    "version": "npm run timestamp; cd src/iframe; yarn version --patch; git push --follow-tags",
    "iframe-start-js": "cd src/iframe; react-scripts start",
    "iframe-watch-css": "cd src/iframe; stylus src/css/Iframe.styl -o src/css -w",
    "iframe-start": "npm-run-all -p iframe-start-js iframe-watch-css",
    "iframe-build": "cd src/iframe; react-scripts build",
    "iframe-test": "cd src/iframe; react-scripts test --env=jsdom",
    "iframe-eject": "cd src/iframe; react-scripts eject",
    "iframe-rename": "cd src/iframe; mv build/index.html build/iframe.html",
    "iframe-copy": "cp -a src/iframe/build/. build/",
    "iframe": "npm-run-all -s iframe-build iframe-rename iframe-copy",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "build-css": "stylus src/css/App.styl -o src/css",
    "watch-css": "stylus src/css/App.styl -o src/css -w",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js watch-css watch-lessons watch-help",
    "build-js": "react-scripts build",
    "build": "npm-run-all -s build-css build-js iframe",
    "serve": "serve -s build",
    "build-dev": "npm-run-all -s build serve",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "standard": {
    "globals": [
      "it",
      "test",
      "describe",
      "expect",
      "localStorage"
    ]
  }
}